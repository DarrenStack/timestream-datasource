{
  "name": "grafana-timestream-datasource",
  "version": "1.2.0-dev",
  "description": "Load data timestream in grafana",
  "scripts": {
    "build": "rm -rf dist && grafana-toolkit plugin:build && mage build:backend",
    "test": "grafana-toolkit plugin:test",
    "test:e2e": "rm -rf node_modules/@grafana/data/node_modules; grafana-e2e run",
    "test:e2e:update": "grafana-e2e run --update-screenshots",
    "dev": "rm -rf node_modules/@grafana/data/node_modules; grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "repository": "github:grafana/timestream-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "7.3.0",
    "@grafana/ui": "7.3.0",
    "@grafana/e2e": "^7.3.0",
    "@grafana/runtime": "7.3.0",
    "@grafana/toolkit": "latest",
    "@types/lodash": "latest"
  }
}
